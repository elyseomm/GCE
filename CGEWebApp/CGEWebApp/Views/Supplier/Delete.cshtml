<script src="~/Content/App/appBase.js"></script>
@model FocusBrazilSFC.Core.ProjectConfig
@{
    ViewBag.Title = TempData["title"];
}
<h2>@ViewBag.Title</h2>
<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>@ViewBag.Title</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.ID_PROJECT)</dt>
        <dd>@Html.DisplayFor(model => model.Project.PROJECT_CODE)</dd>

        <dt>@Html.DisplayNameFor(model => model.NAME)</dt>
        <dd>@Html.DisplayFor(model => model.NAME)</dd>

        <dt>@Html.DisplayNameFor(model => model.VALUE)</dt>
        <dd>@Html.DisplayFor(model => model.VALUE)</dd>

        <dt>@Html.DisplayNameFor(model => model.HASH)</dt>
        <dd>@Html.DisplayFor(model => model.HASH)</dd>

        <dt>@Html.DisplayNameFor(model => model.DATE_CREATE)</dt>
        <dd>@Html.DisplayFor(model => model.DATE_CREATE)</dd>

        <dt>@Html.DisplayNameFor(model => model.ACTIVE)</dt>
        <dd>@Html.DisplayFor(model => model.BACTIVE)</dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>
